<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>firstHttpServletRequest</servlet-name>
        <servlet-class>org.lzn.FirstHttpServletRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>secondHttpServletRequest</servlet-name>
        <servlet-class>org.lzn.SecondHttpServletRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>thirdHttpServletRequest</servlet-name>
        <servlet-class>org.lzn.ThirdHttpServletRequest</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>firstHttpServletRequest</servlet-name>
        <url-pattern>/demo1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>secondHttpServletRequest</servlet-name>
        <url-pattern>/demo2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>thirdHttpServletRequest</servlet-name>
        <url-pattern>/demo3</url-pattern>
    </servlet-mapping>
</web-app>