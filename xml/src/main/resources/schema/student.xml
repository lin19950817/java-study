<?xml version="1.0" encoding="UTF-8" ?>

<!--
    编写根标签
    引入实例名称空间 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    引入名称空间 xsi:schemaLocation="http://www.lzn.cn/xml"
    引入默认的名称空间
-->
<students xmlns="http://www.lzn.cn/xml"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.lzn.cn/xml student.xsd"
>

    <student number="hehe_101">
        <name>hehe</name>
        <age>18</age>
        <sex>male</sex>
    </student>

</students>

