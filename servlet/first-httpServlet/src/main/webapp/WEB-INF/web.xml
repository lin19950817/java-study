<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>First HttpServlet</display-name>

    <servlet>
        <servlet-name>firstHttpServlet</servlet-name>
        <servlet-class>com.lzn.FirstHttpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>secondHttpServlet</servlet-name>
        <servlet-class>com.lzn.SecondHttpServlet</servlet-class>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>GBK</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>firstHttpServlet</servlet-name>
        <url-pattern>/demo1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>secondHttpServlet</servlet-name>
        <url-pattern>/demo2</url-pattern>
    </servlet-mapping>
</web-app>
