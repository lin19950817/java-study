<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
    <param-name>encoding</param-name>
    <param-value>UTF-8</param-value>
  </context-param>

  <servlet>
    <servlet-name>firstServletContext</servlet-name>
    <servlet-class>com.lzn.FirstServletContext</servlet-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>GBK</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>secondSerlvetContext</servlet-name>
    <servlet-class>com.lzn.SecondServletContext</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>thirdServletContext</servlet-name>
    <servlet-class>com.lzn.ThirdServletContext</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>fourthServletContext</servlet-name>
    <servlet-class>com.lzn.FourthServletContext</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>firstServletContext</servlet-name>
    <url-pattern>/demo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>secondSerlvetContext</servlet-name>
    <url-pattern>/demo2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>thirdServletContext</servlet-name>
    <url-pattern>/demo3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fourthServletContext</servlet-name>
    <url-pattern>/demo4</url-pattern>
  </servlet-mapping>
</web-app>
